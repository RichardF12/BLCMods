					<code profiles="default">set GD_Population_Thresher.Balance.Unique.PawnBalance_TentacleRaidF PlayThroughs[0] (PlayThrough=1,DisplayName="Elpisian Terramorphous Doppelganger")</code>
				</hotfix>
			</category>
			<category name="Rename Terramorphous the Invincible to Pandora Shifter the Invincible">
				<hotfix name="Name" level="None">
					<code profiles="">set GD_Population_Thresher.Balance.Unique.PawnBalance_ThresherRaid PlayThroughs[0] (PlayThrough=1,DisplayName="Pandora Shifter the Invincible")</code>
				</hotfix>
			</category>
