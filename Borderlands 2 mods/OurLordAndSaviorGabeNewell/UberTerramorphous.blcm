					<code profiles="default">set GD_Population_Thresher.Balance.Unique.PawnBalance_TentacleRaidC PlayThroughs[0] (PlayThrough=1,DisplayName="Elpisian Terramorphous Doppelganger")</code>
					<code profiles="default">set GD_Population_Thresher.Balance.Unique.PawnBalance_TentacleRaidD AIPawnArchetype WillowAIPawn'GD_Thresher_Raid.Character.Pawn_Thresher_Raid'</code>
					<code profiles="default">set GD_Population_Thresher.Balance.Unique.PawnBalance_TentacleRaidD PlayThroughs[0] (PlayThrough=1,DisplayName="Elpisian Terramorphous Doppelganger")</code>
					<code profiles="default">set GD_Population_Thresher.Balance.Unique.PawnBalance_TentacleRaidE PlayThroughs[0] (PlayThrough=1,DisplayName="Elpisian Terramorphous Doppelganger")</code>
					<code profiles="default">set GD_Population_Thresher.Balance.Unique.PawnBalance_TentacleRaidE AIPawnArchetype WillowAIPawn'GD_Thresher_Raid.Character.Pawn_Thresher_Raid'</code>
